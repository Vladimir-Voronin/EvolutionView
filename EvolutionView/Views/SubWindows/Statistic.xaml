<UserControl x:Class="EvolutionView.Views.SubWindows.Statistic"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EvolutionView.Views.SubWindows"
             xmlns:vm="clr-namespace:EvolutionView.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="830" d:DesignWidth="800">
    <UserControl.Resources>
        <vm:StatisticViewModel x:Key="StatisticViewModel" />
    </UserControl.Resources>
    <Grid Grid.Row="1"
          Grid.Column="0"
          Name="StatisticsPopup"
          HorizontalAlignment="Right"
          VerticalAlignment="Top"
          Width="800"
          Height="830"
          Visibility="Visible"
          Background="LightGray">

    </Grid>
</UserControl>
